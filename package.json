{
    "name": "@aura/aura-glass",
    "version": "1.0.0",
    "description": "AuraOne Glass Design System Components - A comprehensive collection of glassmorphism UI components",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        },
        "./styles": "./dist/styles/index.css",
        "./styles/*": "./dist/styles/*"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "rollup -c",
        "dev": "rollup -c -w",
        "typecheck": "tsc --noEmit",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "format": "prettier --write src/**/*.{ts,tsx,css}",
        "clean": "rm -rf dist",
        "prepublishOnly": "pnpm run clean && pnpm run typecheck && pnpm run build"
    },
    "keywords": [
        "react",
        "glassmorphism",
        "ui-components",
        "design-system",
        "typescript",
        "glass-ui",
        "auraone"
    ],
    "author": "AuraOne Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/auraone/aura-glass.git"
    },
    "peerDependencies": {
        "react": ">=16.8.0",
        "react-dom": ">=16.8.0"
    },
    "devDependencies": {
        "@types/react": "^18.3.24",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.45.0",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "postcss": "^8.4.24",
        "prettier": "^3.0.0",
        "rollup": "^4.0.0",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-typescript2": "^0.36.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "dependencies": {
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-slot": "^1.2.3",
        "chart.js": "^4.5.0",
        "clsx": "^2.1.1",
        "framer-motion": "^12.23.12",
        "lucide-react": "^0.542.0",
        "react-chartjs-2": "^5.3.0",
        "react-hook-form": "^7.62.0",
        "styled-components": "^6.1.19",
        "tailwind-merge": "^3.3.1"
    }
}
